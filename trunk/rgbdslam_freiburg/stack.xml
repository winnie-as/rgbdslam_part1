<stack>
  <description brief="rgbdslam_freiburg">rgbdslam_freiburg contains the RGBD-SLAM application from Uni Freiburg</description>
  <author>Maintained by Felix Endres</author>
  <license>GPL v3</license>  
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rgbdslam_freiburg</url>
  <depend stack="ros" />
  <depend package="tf"/>
  <depend package="pcl"/>
  <depend package="eigen"/>
  <depend package="rospy"/>
  <depend package="roscpp"/>
  <depend package="rosbag"/>
  <depend package="pcl_ros"/>
  <!--depend package="opencv2"/-->
  <depend package="cv_bridge"/>
  <depend package="sensor_msgs"/>
  <!--depend package="openni_camera"/-->
  <depend package="geometry_msgs"/>
  <depend package="visualization_msgs"/>
  <rosdep name="opengl"/>
  <rosdep name="qt4"/>
  <rosdep name="libglew"/>
  <rosdep name="libdevil"/>
  <rosdep name="gsl-dev"/>
  <export>
    <rosdoc config="rosdoc.yaml" />
    <nodelet plugin="${prefix}/nodelet_plugins.xml" />
    <cpp cflags="-I${prefix}/srv_gen/cpp"/>
 </export>

</stack>
